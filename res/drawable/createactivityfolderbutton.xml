<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#FF9900" />
            <stroke
                android:width="1dp"
                android:color="#FF9900" />
            <corners
                android:radius="5dp" />
          
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#bb6008"
                android:endColor="#bb6008"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#bb6008" />
            <corners
                android:radius="6dp" />
           
        </shape>
    </item>
</selector>